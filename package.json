{
    "name": "@pushme-tgxn/pushmesdk",
    "author": "tgxn <pushme@tgxn.net>",
    "keywords": [
        "javascript",
        "cli",
        "console",
        "sdk"
    ],
    "version": "1.7.2",
    "repository": "https://github.com/pushme-tgxn/PushMeSDK",
    "main": "./dist/node/index.cjs",
    "module": "./dist/node/index.mjs",
    "scripts": {
        "bundle": "rollup -c",
        "postbundle": "cp ./src/index.d.ts ./dist/node/index.d.ts && cp ./src/index.d.ts ./dist/browser/index.d.ts",
        "test": "run-s -n test-cjs test-esm",
        "test-cjs": "mocha test/test-sdk-cjs.cjs",
        "test-esm": "mocha test/test-sdk-esm.mjs",
        "coverage-cjs": "nyc mocha test/test-sdk-cjs.cjs",
        "coverage-esm": "nyc mocha test/test-sdk-esm.mjs",
        "coverage": "run-s -n coverage-cjs coverage-esm"
    },
    "exports": {
        ".": {
            "types": {
                "require": "./src/index.d.ts",
                "default": "./src/index.d.ts"
            },
            "browser": {
                "require": "./dist/browser/index.cjs",
                "default": "./dist/browser/index.mjs"
            },
            "react-native": {
                "require": "./dist/browser/index.cjs",
                "default": "./dist/browser/index.mjs"
            },
            "default": {
                "require": "./dist/node/index.cjs",
                "default": "./dist/node/index.mjs"
            }
        },
        "./package.json": "./package.json"
    },
    "files": [
        "src/**",
        "dist/**"
    ],
    "license": "MIT",
    "dependencies": {
        "axios": "^1.2.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@faker-js/faker": "^7.6.0",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "chai": "^4.3.7",
        "mocha": "^10.2.0",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "prettier": "^2.8.1",
        "rollup": "^3.9.1",
        "rollup-plugin-auto-external": "^2.0.0",
        "rollup-plugin-node-externals": "^5.0.3"
    }
}
